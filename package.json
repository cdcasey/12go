{
  "name": "12go-website",
  "private": true,
  "description": "Website for the podcast One to Grow On",
  "version": "0.1.0",
  "author": "Chris Casey @cdcasey",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "dotenv": "^8.2.0",
    "gatsby": "^2.23.3",
    "gatsby-cli": "^2.12.45",
    "gatsby-image": "^2.4.7",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-google-analytics": "^2.3.4",
    "gatsby-plugin-manifest": "^2.4.11",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-plugin-styled-components": "^3.3.4",
    "gatsby-plugin-twitter": "^2.3.4",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-source-wordpress": "^3.3.11",
    "gatsby-transformer-sharp": "^2.5.5",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn git"
    }
  },
  "keywords": [
    "gatsby",
    "One to Grow On",
    "podcasts",
    "agriculture"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,css}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "eslint --ignore-path .gitignore .",
    "git": "yarn outdated",
    "deploy": "git pull && yarn install && yarn build && rsync -avhW --delete-after public/ ../public/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdcasey/12go"
  },
  "bugs": {
    "url": "https://github.com/cdcasey/12go/issues"
  }
}
