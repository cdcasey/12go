{
  "name": "12go-website",
  "private": true,
  "description": "Website for the podcast One to Grow On",
  "version": "0.1.0",
  "author": "Chris Casey @cdcasey",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.3",
    "gatsby-cli": "^2.7.42",
    "gatsby-image": "^2.2.16",
    "gatsby-paginate": "^1.1.0",
    "gatsby-plugin-manifest": "^2.2.13",
    "gatsby-plugin-offline": "^3.0.1",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-sass": "^2.1.13",
    "gatsby-plugin-sharp": "^2.2.19",
    "gatsby-plugin-twitter": "^2.1.5",
    "gatsby-source-filesystem": "^2.1.19",
    "gatsby-source-wordpress": "^3.1.28",
    "gatsby-transformer-sharp": "^2.2.13",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn git"
    }
  },
  "keywords": [
    "gatsby",
    "One to Grow On",
    "podcasts",
    "agriculture"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "git": "yarn outdated",
    "deploy": "git pull && yarn install && yarn build && rsync -avhW --delete-after public/ ../public/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdcasey/12go"
  },
  "bugs": {
    "url": "https://github.com/cdcasey/12go/issues"
  }
}
