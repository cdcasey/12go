{
  "name": "12go-website",
  "private": true,
  "description": "Website for the podcast One to Grow On",
  "version": "0.1.0",
  "author": "Chris Casey @cdcasey",
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.67",
    "gatsby-cli": "^2.12.102",
    "gatsby-image": "^2.4.20",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-google-analytics": "^2.3.15",
    "gatsby-plugin-manifest": "^2.4.33",
    "gatsby-plugin-offline": "^3.2.30",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sass": "^2.3.14",
    "gatsby-plugin-sharp": "^2.6.38",
    "gatsby-plugin-styled-components": "^3.3.12",
    "gatsby-plugin-twitter": "^2.3.12",
    "gatsby-source-filesystem": "^2.3.32",
    "gatsby-source-wordpress": "^3.3.36",
    "gatsby-transformer-sharp": "^2.5.16",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn git"
    }
  },
  "keywords": [
    "gatsby",
    "One to Grow On",
    "podcasts",
    "agriculture"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|json|css|scss)\"",
    "format": "yarn prettier -- --write",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .gitignore .",
    "git": "yarn lint && yarn outdated",
    "deploy": "git pull && yarn install && yarn build && rsync -avhW --delete-after public/ ../public/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdcasey/12go"
  },
  "bugs": {
    "url": "https://github.com/cdcasey/12go/issues"
  }
}
